{
    "name": "denismitr/laravel-permissions",
    "description": "Laravel Roles And Permissions Package with Teams support",
    "keywords": ["denismitr", "roles", "permissions", "teams", "acl", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mitrofanov",
            "email": "denis.mitr@gmail.com",
            "homepage": "https://thecollection.ru",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Denismitr\\LTP\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Denismitr\\LTP\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/console": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/auth": "~5.5.0|~5.6.0",
        "illuminate/container": "~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.5.0|~5.6.0",
        "illuminate/events": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5.0|^3.6.0",
        "phpunit/phpunit" : "^7.0|^7.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denismitr\\LTP\\LTPServiceProvider"
            ]
        }
    }
}
